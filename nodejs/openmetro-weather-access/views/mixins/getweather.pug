mixin getWeatherIcon(weather)
  if weather=='sunny'
    include ../weather-icons/sunny
  else if weather == 'snowy'
    include ../weather-icons/snowy
  else if weather == 'rainy'
    include ../weather-icons/rainy
  else if weather == 'cloudy'
    include ../weather-icons/cloudy
  else if weather == 'stormy'
    include ../weather-icons/stormy
  else if weather == 'thunder'
    include ../weather-icons/thunder
  else if weather == 'moon'
    include ../weather-icons/moon
  else
    p WeatherImage
    p= weather
